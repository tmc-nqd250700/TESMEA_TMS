<UserControl x:Class="TESMEA_TMS.Views.ProjectView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TESMEA_TMS.Views"
             xmlns:lex="http://wpflocalizeextension.codeplex.com"
             lex:LocalizeDictionary.DesignCulture="vi"
             lex:ResxLocalizationProvider.DefaultAssembly="TESMEA_TMS"
             lex:ResxLocalizationProvider.DefaultDictionary="Strings"
             mc:Ignorable="d" 
             DataContext="{Binding ProjectViewModel, Source={StaticResource Locator}}">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Margin="10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <ScrollViewer Grid.Column="0" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <GroupBox Grid.Row="0" Header="{lex:Loc groupbox_Parameter}" FontSize="13" Margin="0 0 0 10">
                        <UniformGrid Columns="2">
                            <TextBlock Text="{lex:Loc lbl_Project_name}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding ThongTinDuAn.ThamSo.TenDuAn, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <TextBlock Text="{lex:Loc lbl_Scenario}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <ComboBox Cursor="Hand"
                                      md:ComboBoxAssist.ShowSelectedItem="True"
                                      Style="{StaticResource MaterialDesignComboBox}" 
                                      ItemsSource="{Binding ScenarioTypes}" 
                                      SelectedValue="{Binding ThongTinDuAn.ThamSo.KichBan, Mode=TwoWay}"
                                      SelectedValuePath="Value"
                                      DisplayMemberPath="Text"/>

                            <TextBlock Text="{lex:Loc lbl_Library_type}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <ComboBox Cursor="Hand"
                                      md:ComboBoxAssist.ShowSelectedItem="True"
                                      Style="{StaticResource MaterialDesignComboBox}" 
                                      ItemsSource="{Binding LibTypes}" 
                                      SelectedValue="{Binding ThongTinDuAn.ThamSo.ThongSo, Mode=TwoWay}"
                                      SelectedValuePath="Value"
                                      DisplayMemberPath="Text"
                                      SelectionChanged="ComboBox_SelectionChanged"/>

                            <TextBlock Text="{lex:Loc lbl_Test_type}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <ComboBox Cursor="Hand"
                                      md:ComboBoxAssist.ShowSelectedItem="True"
                                      Style="{StaticResource MaterialDesignComboBox}" 
                                      ItemsSource="{Binding TestTypes}" 
                                      SelectedValue="{Binding ThongTinDuAn.ThamSo.KieuKiemThu, Mode=TwoWay}"
                                      SelectedValuePath="Value"
                                      DisplayMemberPath="Text"
                                      SelectionChanged="ComboBox_SelectionChanged"/>

                            <TextBlock Text="{lex:Loc lbl_Project_path}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox md:HintAssist.Hint="{lex:Loc hint_Project_path}" Cursor="Hand"  PreviewMouseLeftButtonDown="TextBox_PreviewMouseLeftButtonDown" Text="{Binding ThamSo.DuongDanLuuDuAn, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                     IsReadOnly="True"/>
                        </UniformGrid>
                    </GroupBox>
                    
                    <GroupBox Grid.Row="1" Header="{lex:Loc groupbox_General_infomation}" Margin="0 0 0 10" FontSize="13">
                        <UniformGrid Columns="2">
                            <TextBlock Text="{Binding Source={lex:Loc lbl_Sample}, StringFormat={}1. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding ThongTinChung.TenMauThu, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Manufacturer}, StringFormat={}2. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding ThongTinChung.CoSoSanXuat, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Sample_symbol}, StringFormat={}3. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding ThongTinChung.KyHieu, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Quantity}, StringFormat={}4. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox PreviewTextInput="TextBox_PreviewTextInput" 
                                         PreviewKeyDown="TextBox_PreviewKeyDown" 
                                         TextChanged="TextBox_TextChanged"
                                         Text="{Binding ThongTinChung.SoLuongMau, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Sample_obser}, StringFormat={}5. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox Text="{Binding ThongTinChung.TinhTrangMau, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Request_date}, StringFormat={}6. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>

                            <DatePicker Style="{StaticResource MaterialDesignDatePicker}" 
                                             SelectedDate="{Binding ThongTinChung.NgayNhanYeuCau, Mode=TwoWay, StringFormat=dd/MM/yyyy}"
                                             SelectedDateFormat="Short"  Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Reception_date}, StringFormat={}7. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <DatePicker Style="{StaticResource MaterialDesignDatePicker}" 
                                             SelectedDate="{Binding ThongTinChung.NgayNhanMau, Mode=TwoWay, StringFormat=dd/MM/yyyy}"
                                             SelectedDateFormat="Short"  Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Test_duration}, StringFormat={}8. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <DatePicker Style="{StaticResource MaterialDesignDatePicker}" 
                                             SelectedDate="{Binding ThongTinChung.NgayThuNghiem, Mode=TwoWay, StringFormat=dd/MM/yyyy}"
                                             SelectedDateFormat="Short"  Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Test_completed_date}, StringFormat={}9. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <DatePicker Style="{StaticResource MaterialDesignDatePicker}"
                                             SelectedDate="{Binding ThongTinChung.NgayHoanThanh, Mode=TwoWay, StringFormat=dd/MM/yyyy}"
                                             SelectedDateFormat="Short"  Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Text="{Binding Source={lex:Loc lbl_Applied_standard}, StringFormat={}10. {0}}" Margin="10 0 5 0" VerticalAlignment="Center"/>
                            <TextBox VerticalAlignment="Top"
                                         AcceptsReturn="True" 
                                         TextWrapping="Wrap" 
                                         Text="{Binding ThongTinChung.TieuChuanApDung, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                        </UniformGrid>
                    </GroupBox>

                    <GroupBox Grid.Row="2" Header="{lex:Loc groupbox_Sample_information}" Margin=" 0 0 0 10" FontSize="13">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Grid.Column="0" 
                                       Text="{Binding Source={lex:Loc lbl_Airflow_design}, StringFormat={}1. {0} (m³/h)}" VerticalAlignment="Center"
                                       Margin="10 0 5 0" />
                            <TextBox Grid.Row="0" Grid.Column="1" Text="{Binding ThongTinMauThuNghiem.LuuLuongKhiThietKe, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                            
                            <TextBlock Grid.Row="1" Grid.Column="0" 
                                       Text="{Binding Source={lex:Loc lbl_SPressure_design}, StringFormat={}2. {0} (Pa)}" VerticalAlignment="Center"
                                       Margin="10 0 5 0" />
                            <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding ThongTinMauThuNghiem.ApSuatThietKe, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Grid.Row="2" Grid.Column="0" 
                                       Text="{Binding Source={lex:Loc lbl_Temp_fluid}, StringFormat={}3. {0} (ºC)}" VerticalAlignment="Center"
                                       Margin="10 0 5 0" />
                            <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding ThongTinMauThuNghiem.NhietDoThietKeLamViec, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>

                            <TextBlock Grid.Row="3" Grid.Column="0" Text="{Binding Source={lex:Loc lbl_Motor_power}, StringFormat={}4. {0} (kW)}" TextWrapping="Wrap" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <StackPanel Grid.Row="3" Grid.Column="1" Orientation="Horizontal">
                                <TextBox Text="{Binding ThongTinMauThuNghiem.CongSuatDongCo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                                <Button x:Name="btnPw_i" Margin="20 0 0 0" Width="48" Height="Auto"  Style="{StaticResource SolidButtonStyle}" Content="?" Padding="0" FontSize="15" Click="btnPw_i_Click"/>
                            </StackPanel>

                            <TextBlock Grid.Row="4" Grid.Column="0" Margin="10 0 5 0" Text="{Binding Source={lex:Loc lbl_Fan_speed}, StringFormat={}5. {0} (rpm)}" TextWrapping="Wrap" VerticalAlignment="Center" />
                            <StackPanel Grid.Row="4" Grid.Column="1" Orientation="Horizontal">
                            <TextBox Text="{Binding ThongTinMauThuNghiem.TocDoThietKeCuaQuat, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                                <Button x:Name="btnSpeed_i" Margin="20 0 0 0" Width="48" Height="Auto"  Style="{StaticResource SolidButtonStyle}" Content="?" Padding="0" FontSize="15" Click="btnSpeed_i_Click"/>
                            </StackPanel>

                            <TextBlock Grid.Row="5" Grid.Column="0" 
                                       Text="{Binding Source={lex:Loc lbl_Frequency_design}, StringFormat={}6. {0} (Hz)}" VerticalAlignment="Center"
                                       Margin="10 0 5 0" />
                            <TextBox Grid.Row="5" Grid.Column="1" Text="{Binding ThongTinMauThuNghiem.TanSoDongCoTheoThietKe, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                                <TextBlock Grid.Row="6" Grid.Column="0" Margin="10 0 5 0" Text="{Binding Source={lex:Loc lbl_Motor_cosphi}, StringFormat={}7. {0}}" TextWrapping="Wrap" VerticalAlignment="Center"/>

                            <StackPanel Grid.Row="6" Grid.Column="1" Orientation="Horizontal">
                                <TextBox Text="{Binding ThongTinMauThuNghiem.HeSoCongSuatDongCo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                                <Button Margin="20 0 0 0" x:Name="btnCosphi_i" Width="48" Height="Auto"  Style="{StaticResource SolidButtonStyle}" Content="?" Padding="0" FontSize="15" Click="btnCosphi_i_Click"/>
                            </StackPanel>

                                
                            <TextBlock Grid.Row="7" Grid.Column="0" Margin="10 0 5 0" Text="{Binding Source={lex:Loc lbl_Motor_eff}, StringFormat={}8. {0} (%)}" TextWrapping="Wrap" VerticalAlignment="Center"/>
                            <StackPanel  Grid.Row="7" Grid.Column="1" Orientation="Horizontal" >
                                <TextBox Text="{Binding ThongTinMauThuNghiem.HieuSuatDongCo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                                <Button Margin="20 0 0 0" x:Name="btnEff_i" Width="48" Height="Auto"  Style="{StaticResource SolidButtonStyle}" Content="?" Padding="0" FontSize="15" Click="btnEff_i_Click"/>
                            </StackPanel>

                            <TextBlock Grid.Row="8" Grid.Column="0" Margin="10 0 5 0" Text="{Binding Source={lex:Loc lbl_Rated_current}, StringFormat={}9. {0} (A)}" TextWrapping="Wrap" VerticalAlignment="Center"/>
                            <StackPanel Grid.Row="8" Grid.Column="1" Orientation="Horizontal">
                                <TextBox  Text="{Binding ThongTinMauThuNghiem.DongDienDinhMucCuaDongCo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                                <Button Margin="20 0 0 0" x:Name="btnIdm_i" Width="48" Height="Auto"  Style="{StaticResource SolidButtonStyle}" Content="?" Padding="0" FontSize="15" Click="btnIdm_i_Click"/>
                            </StackPanel>

                            <TextBlock Grid.Row="9" Grid.Column="0" Margin="10 0 5 0" Text="{Binding Source={lex:Loc lbl_Motor_voltage}, StringFormat={}10. {0} (V)}" TextWrapping="Wrap" VerticalAlignment="Center"/>
                            <StackPanel  Grid.Row="9" Grid.Column="1" Orientation="Horizontal">
                            <TextBox  Text="{Binding ThongTinMauThuNghiem.DienApDongCo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Width="100" HorizontalAlignment="Left"/>
                                <Button Margin="20 0 0 0" x:Name="btnUdm_i" Width="48" Height="Auto"  Style="{StaticResource SolidButtonStyle}" Content="?" Padding="0" FontSize="15" Click="btnUdm_i_Click"/>
                            </StackPanel>

                            <TextBlock Grid.Row="10" Grid.Column="0" 
                                       Text="11. Độ nhớt không khí" VerticalAlignment="Center"
                                       Margin="10 0 5 0" />
                            <TextBox Grid.Row="10" Grid.Column="1" 
                                     IsEnabled="False"
                                     Text="{Binding ThongTinMauThuNghiem.DoNhotKhongKhi, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, StringFormat={}{0:N7}}" Width="100" HorizontalAlignment="Left"/>
                        </Grid>
                    </GroupBox>
                </Grid>
            </ScrollViewer>
            <md:PackIcon Grid.Column="1" Kind="ArrowRightBold" Width="40" Height="40" Foreground="DodgerBlue" VerticalAlignment="Center" Margin="10" HorizontalAlignment="Center"/>
            <ScrollViewer Grid.Column="2" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                   
                    <GroupBox Grid.Row="0" Header="{lex:Loc groupbox_Inverter}" FontSize="13" Margin="0 0 0 10">
                        <Grid IsEnabled="False">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="100"/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Text="Điện áp vào (V)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="0" Grid.Column="1" Text="{Binding BienTan.DienApVao, Mode=OneWay}" />
                            <TextBlock Grid.Row="0" Grid.Column="2" Text="Điện áp ra (V)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="0" Grid.Column="3" Text="{Binding BienTan.DienApRa, Mode=OneWay}"  />

                            <TextBlock Grid.Row="1" Grid.Column="0" Text="Dòng điện vào (A)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding BienTan.DongDienVao, Mode=OneWay}"  />
                            <TextBlock Grid.Row="1" Grid.Column="2" Text="Dòng điện ra (A)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="1" Grid.Column="3" Text="{Binding BienTan.DongDienRa, Mode=OneWay}"  />

                            <TextBlock Grid.Row="2" Grid.Column="0" Text="Tần số vào (Hz)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding BienTan.TanSoVao, Mode=OneWay}"  />
                            <TextBlock Grid.Row="2" Grid.Column="2" Text="Tần số ra (Hz)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="2" Grid.Column="3" Text="{Binding BienTan.TanSoRa, Mode=OneWay}"  />

                            <TextBlock Grid.Row="3" Grid.Column="0" Text="Công suất vào (kVA)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="3" Grid.Column="1" Text="{Binding BienTan.CongSuatVao, Mode=OneWay}"  />
                            <TextBlock Grid.Row="3" Grid.Column="2" Text="Công suất tổng ra (kVA)" TextWrapping="Wrap" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="3" Grid.Column="3" Text="{Binding BienTan.CongSuatTongRa, Mode=OneWay}"  />

                            <TextBlock Grid.Row="4" Grid.Column="0" Text="Hiệu suất bộ truyền (%)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="4" Grid.Column="1" Text="{Binding BienTan.HieuSuatBoTruyen, Mode=OneWay}"  />
                            
                            <TextBlock Grid.Row="4" Grid.Column="2" Text="Công suất hiệu dụng ra (kVA)" TextWrapping="Wrap" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="4" Grid.Column="3" Text="{Binding BienTan.CongSuatHieuDungRa, Mode=OneWay}"  />

                            <TextBlock Grid.Row="5" Grid.Column="0" Text="Hiệu suất của khớp nối trục đàn hồi (%)" TextWrapping="Wrap" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="5" Grid.Column="1" Text="{Binding BienTan.HieuSuatNoiTruc, Mode=OneWay}"  />
                            
                            <TextBlock Grid.Row="5" Grid.Column="2" Text="Công suất gối trục (%)" TextWrapping="Wrap" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="5" Grid.Column="3" Text="{Binding BienTan.HieuSuatGoiTruc, Mode=OneWay}"  />
                        </Grid>
                    </GroupBox>
                    
                    <GroupBox Grid.Row="1" Header="{lex:Loc groupbox_Duct}" FontSize="13" Margin="0 0 0 10">
                        <Grid IsEnabled="False">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="100"/>
                            </Grid.ColumnDefinitions>

                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>

                            <TextBlock Grid.Row="0" Grid.Column="0" Text="Đường kính ống D5" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="0" Grid.Column="1" Text="{Binding OngGio.DuongKinhOngD5, Mode=TwoWay}" />

                            <TextBlock Grid.Row="0" Grid.Column="2" Text="Chiều dài côn quạt" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="0" Grid.Column="3" Text="{Binding OngGio.ChieuDaiConQuat, Mode=TwoWay}" />

                            <TextBlock Grid.Row="1" Grid.Column="0" Text="Đường kính ống D3" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding OngGio.DuongKinhOngD3, Mode=TwoWay}" />

                            <TextBlock Grid.Row="1" Grid.Column="2" Text="Đường kính lỗ phụt" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="1" Grid.Column="3" Text="{Binding OngGio.DuongKinhLoPhut, Mode=TwoWay}" />

                            <TextBlock Grid.Row="2" Grid.Column="0" Text="Tiết diện ống D5" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="2" Grid.Column="1" Text="{Binding OngGio.TietDienOngD5, Mode=TwoWay, StringFormat={}{0:N3}}" />

                            <TextBlock Grid.Row="2" Grid.Column="2" Text="Hệ số ma sát" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="2" Grid.Column="3" Text="{Binding OngGio.HeSoMaSat, Mode=TwoWay}" />

                            <TextBlock Grid.Row="3" Grid.Column="0" Text="Tiết diện ống D3" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="3" Grid.Column="1" Text="{Binding OngGio.TietDienOngD3, Mode=TwoWay, StringFormat={}{0:N3}}" />

                            <TextBlock Grid.Row="3" Grid.Column="2" Text="Đường kính miệng quạt" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <TextBox Grid.Row="3" Grid.Column="3" Text="{Binding OngGio.DuongKinhMiengQuat, Mode=TwoWay}" />
                        </Grid>
                    </GroupBox>

                    <GroupBox Grid.Row="2" Header="{lex:Loc groupbox_Sensor}" FontSize="13">
                        <Grid Margin="5 10 0 5" IsEnabled="False">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="0" Grid.Column="0" Text="{lex:Loc lbl_Sensor_range}" VerticalAlignment="Center" Margin="10 0 5 0" FontWeight="SemiBold"/>
                            <TextBlock Grid.Row="0" Grid.Column="1" Text="Nhập tay" VerticalAlignment="Center" Margin="10 0 5 5" FontWeight="SemiBold" HorizontalAlignment="Center"/>
                            <TextBlock Grid.Row="0" Grid.Column="2" Text="Value" VerticalAlignment="Center" Margin="10 0 5 5" FontWeight="SemiBold" HorizontalAlignment="Center"/>
                            <TextBlock Grid.Row="0" Grid.Column="3" Text="Zero" VerticalAlignment="Center" Margin="10 0 5 5" FontWeight="SemiBold" HorizontalAlignment="Center"/>
                            <TextBlock Grid.Row="0" Grid.Column="4" Text="Span" VerticalAlignment="Center" Margin="10 0 5 5" FontWeight="SemiBold" HorizontalAlignment="Center"/>

                            <!-- 1. Cảm biến nhiệt độ môi trường (°C) -->
                            <TextBlock Grid.Row="1" Grid.Column="0" Text="1. Cảm biến nhiệt độ môi trường (°C)" TextWrapping="Wrap" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <CheckBox x:Name="chkImportNhietDoMoiTruong" Grid.Row="1" Grid.Column="1" Margin="10 0 5 0" IsChecked="{Binding CamBien.IsImportNhietDoMoiTruong, Mode=TwoWay}" HorizontalAlignment="Center"/>
                            <TextBox Grid.Row="1" Grid.Column="2" Margin="10 0 5 0" Text="{Binding CamBien.NhietDoMoiTruongValue, Mode=TwoWay}" IsEnabled="{Binding IsChecked, ElementName=chkImportNhietDoMoiTruong}"/>
                            <TextBox Grid.Row="1" Grid.Column="3" Margin="10 0 5 0" Text="{Binding CamBien.NhietDoMoiTruongMin, Mode=TwoWay}" IsEnabled="{Binding IsChecked, ElementName=chkImportNhietDoMoiTruong, Converter={StaticResource BooleanNegationConverter}}"/>
                            <TextBox Grid.Row="1" Grid.Column="4" Margin="10 0 5 0" Text="{Binding CamBien.NhietDoMoiTruongMax, Mode=TwoWay}" IsEnabled="{Binding IsChecked, ElementName=chkImportNhietDoMoiTruong, Converter={StaticResource BooleanNegationConverter}}"/>

                            <!-- 2. Cảm biến độ ẩm môi trường (%) -->
                            <TextBlock Grid.Row="2" Grid.Column="0" Text="2. Cảm biến độ ẩm môi trường (%)" VerticalAlignment="Center" Margin="10 0 5 0"/>
                            <CheckBox x:Name="chkImportDoAmMoiTruong" Grid.Row="2" Grid.Column="1" Margin="10 0 5 0" IsChecked="{Binding CamBien.IsImportDoAmMoiTruong, Mode=TwoWay}" HorizontalAlignment="Center"/>
                            <TextBox Grid.Row="2" Grid.Column="2" Margin="10 0 5 0" Text="{Binding CamBien.DoAmMoiTruongValue, Mode=TwoWay}" IsEnabled="{Binding IsChecked, ElementName=chkImportDoAmMoiTruong}"/>
                            <TextBox Grid.Row="2" Grid.Column="3" Margin="10 0 5 0" Text="{Binding CamBien.DoAmMoiTruongMin, Mode=TwoWay}" IsEnabled="{Binding IsChecked, ElementName=chkImportDoAmMoiTruong, Converter={StaticResource BooleanNegationConverter}}"/>
                            <TextBox Grid.Row="2" Grid.Column="4" Margin="10 0 5 0" Text="{Binding CamBien.DoAmMoiTruongMax, Mode=TwoWay}" IsEnabled="{Binding IsChecked, ElementName=chkImportDoAmMoiTruong, Converter={StaticResource BooleanNegationConverter}}"/>

